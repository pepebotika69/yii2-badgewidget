	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension",
		"widget",
		"badges"
	],
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"pozitronik/yii2-helpers": "dev-master",
		"pozitronik/yii2-cachedwidget": "dev-master"
